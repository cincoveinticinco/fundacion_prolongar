<%=form_with(model: @admin, local: true) do |form| %>

    <div>
        <%=form.label :name %>
        <%=form.text_field :name %>
    </div>
    <div>
        <%=form.label :direccion %>
        <%=form.text_field :direccion %>
    </div>
    <div>
        <%=form.label :edificio %>
        <%=form.text_field :edificio %>
    </div>
    <div>
        <%=form.label :ciudad %>
        <%=form.text_field :ciudad %>
    </div>
    <div>
        <%=form.label :telefono %>
        <%=form.text_field :telefono %>
    </div>
    <div>
        <%=form.label :email %>
        <%=form.text_field :email %>
    </div>
    <div>
        <%=form.submit %>
    </div>


<%end%>